{
  "name": "nauka-gita-3",
  "version": "1.0.0",
  "description": "",
  "main": "index.js",
  "scripts": {
    "test": "npm run test:html && npm run test:js",
    "test:html": "globstar nu-html-checker *.html",
    "test:js": "globstar jshint js/*.js",
    "init-project": "npm install && npm run init:dirs && npm run init:files && npm run init:gitignore",
    "init:dirs": "mkdirp sass css vendor images js",
    "init:files": "touch README.md index.html sass/style.scss js/script.js",
    "init:gitignore": "curl https://raw.githubusercontent.com/github/gitignore/master/Node.gitignore -o .gitignore",
    "build": "npm run build:clean && npm run build:sass && npm run build:autoprefixer && npm run test",
    "build:clean": "rimraf css/*.css",
    "build:sass": "node-sass --output-style expanded --source-map true -o css scss",
    "build:autoprefixer": "autoprefixer-cli css/style.css",
    "watch": "npm-run-all build -p watch:*",
    "watch:sassprefixer": "onchange scss/*.scss -- npm-run-all sassprefixer:*",
    "sassprefixer:sass": "node-sass --output-style expanded --source-map true -o css scss",
    "sassprefixer:autoprefixer": "autoprefixer-cli css/style.css",
    "watch:browser-sync": "browser-sync start --server --files 'css/*.css' '*.html'"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/katarzynka1234/nauka-gita-4.git"
  },
  "keywords": [],
  "author": "",
  "license": "ISC",
  "bugs": {
    "url": "https://github.com/katarzynka1234/nauka-gita-4/issues"
  },
  "homepage": "https://github.com/katarzynka1234/nauka-gita-4#readme",
  "devDependencies": {
    "autoprefixer-cli": "^1.0.0",
    "browser-sync": "^2.23.6",
    "globstar": "^1.0.0",
    "jshint": "^2.9.5",
    "npm-run-all": "^4.1.2",
    "nu-html-checker": "^0.1.0",
    "onchange": "^3.3.0"
  },
  "dependencies": {
    "mkdirp": "^0.5.1",
    "node-sass": "^4.8.3",
    "rimraf": "^2.6.2"
  }
}
